<div class='prefecture-btn'>
     <%= link_to image_tag('add.png',width: 50) , new_prefecture_path %>
</div>

<div class = "prefectures">
   <% current_user.prefectures.each do |prefecture| %>
      <div class="prefecture-name">
       <%= link_to image_tag('bag.png',width: 80), prefecture_places_path(prefecture), class: "prefecture-bag" %>
       <%= link_to prefecture.name, prefecture_places_path(prefecture), class: "prefecture-title"%>
      </div>
  <% end %>
</div>

