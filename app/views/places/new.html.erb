<%= form_with model: [@prefecture, @place], class: 'form', local: true do |f| %>
  <div class="form-input">
    <%= f.text_field :name, class: 'form-place', placeholder: '行先名を入力してください' %>
    <%= f.text_field :url, class: 'form-url', placeholder: 'URLを入力してください' %>
  </div>
  <%= f.submit '送信', class: 'form-submit' %>
<% end %>