<%= link_to "pinを作成する", new_prefecture_pin_path%>
<div class="pins">
 <% @pins.each do |pin| %>
    <div class ="pin-name">
      <%= link_to pin.name, pin.url%>
      <%= image_tag pin.image.variant(resize: '300x300'), class: 'pin-image' if pin.image.attached? %>
    </div>
  <% end %>
</div>




<%= link_to "行先を作成する", new_prefecture_place_path%>
<div class="places">
 <% @places.each do |place| %>
    <div class="place-user">
      <%= place.user.name %>
    </div>
    <div class ="place-name">
      <%= link_to place.name, place.url%>
    </div>
     <%= image_tag place.image.variant(resize: '300x300'), class: 'place-image' if place.image.attached? %>
  <% end %>
</div>