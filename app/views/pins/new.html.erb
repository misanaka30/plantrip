<%= form_with model: [@prefecture, @pin], class: 'form', local: true do |f| %>
  <div class="form-input">
    <%= f.text_field :name, class: 'form-pin', placeholder: 'タイトルを入力してください' %>
    <%= f.text_field :url, class: 'form-url', placeholder: 'URLを入力してください' %>
  </div>
  <label class="form-image">
      <span class="image-file">画像</span>
      <%= f.file_field :image, class: 'hidden' %>
    </label>
  <%= f.submit '送信', class: 'form-submit' %>
<% end %>